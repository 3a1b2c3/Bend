@Len = ({2 {2 * @Len$C0} {2 #0 a}} a)

@Len$C0 = {2 a b}
  & #1 ~ <+ c b>
  & @Len ~ (a c)

@List.cons = (a (b {2 {2 a {2 b c}} {2 * c}}))

@List.nil = {2 * {2 a a}}

@main = a
  & @Len ~ (b a)
  & @List.cons ~ (* (c b))
  & @List.cons ~ (* (d c))
  & @List.cons ~ (* (e d))
  & @List.cons ~ (* (f e))
  & @List.cons ~ (* (g f))
  & @List.cons ~ (* (@List.nil g))
