@Len = ({4 @Len$C0 {4 #0 a}} a)

@Len$C0 = {4 * {4 a c}}
  & #1 ~ <+ b c>
  & @Len ~ (a b)

@List.cons = (a (b {4 {4 a {4 b c}} {4 * c}}))

@List.nil = {4 * {4 a a}}

@main = g
  & @Len ~ (f g)
  & @List.cons ~ (* (e f))
  & @List.cons ~ (* (d e))
  & @List.cons ~ (* (c d))
  & @List.cons ~ (* (b c))
  & @List.cons ~ (* (a b))
  & @List.cons ~ (* (@List.nil a))
