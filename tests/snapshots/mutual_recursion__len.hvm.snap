@Len = ({4 @Len$C0 {4 #0 a}} a)

@Len$C0 = {4 * {4 a b}}
  & #1 ~ <+ c b>
  & @Len ~ (a c)

@List.cons = (a (b {4 {4 a {4 b c}} {4 * c}}))

@List.nil = {4 * {4 a a}}

@main = a
  & @Len ~ (b a)
  & @List.cons ~ (* (c b))
  & @List.cons ~ (* (d c))
  & @List.cons ~ (* (e d))
  & @List.cons ~ (* (f e))
  & @List.cons ~ (* (g f))
  & @List.cons ~ (* (@List.nil g))
