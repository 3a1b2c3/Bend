E =
  $ (0 * (0 * (0 x0 x0)))

I =
  $ (0 x0 (0 * (0 (0 x0 x1) (0 * x1))))

O =
  $ (0 x0 (0 (0 x0 x1) (0 * (0 * x1))))

c2 =
  $ (0 (1 (0 x0 x1) (0 x2 x0)) (0 x2 x1))

Ex2 =
  $ x0
  & (0 x1 x0)
  ~ @run
  & (0 @I (0 @E x1))
  ~ @c2

dec =
  $ (0 (0 @decO (0 @decI (0 @E x0))) x0)

low =
  $ (0 (0 @lowO (0 @lowI (0 @E x0))) x0)

run =
  $ (0 (0 @runO (0 @runI (0 @E x0))) x0)

decI =
  $ (0 x0 x1)
  & (0 x0 x1)
  ~ @low

decO =
  $ (0 x0 x1)
  & (0 x2 x1)
  ~ @I
  & (0 x0 x2)
  ~ @dec

lowI =
  $ (0 x0 x1)
  & (0 x2 x1)
  ~ @O
  & (0 x0 x2)
  ~ @I

lowO =
  $ (0 x0 x1)
  & (0 x2 x1)
  ~ @O
  & (0 x0 x2)
  ~ @O

runI =
  $ (0 x0 x1)
  & (0 x2 x1)
  ~ @run
  & (0 x3 x2)
  ~ @dec
  & (0 x0 x3)
  ~ @I

runO =
  $ (0 x0 x1)
  & (0 x2 x1)
  ~ @run
  & (0 x3 x2)
  ~ @dec
  & (0 x0 x3)
  ~ @O

