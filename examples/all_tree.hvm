data Bool = Bool/true | Bool/false

data Tree
  = (Tree/node ~lft ~rgt) 
  | (Tree/leaf val)

and a b = match a {
  true: b
  false: false
}

all tree = fold tree {
  Tree/node: (and tree.lft tree.rgt)
  Tree/leaf: tree.val
}

gen n = switch n {
  0: (Tree/leaf Bool/true)
  _: (Tree/node (gen n-1) (gen n-1))
}

main = (all (gen 8))

